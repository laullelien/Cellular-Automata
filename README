Vous pouvez tester nos class avec les commandes suivantes:
Balls:
	make runTestBallsSimulator

GameOfLife:
    make runStandardCells
    make runStaticCells
    make runTwoStatesCells
    make runCellsCenConvergeQuickly
    make runCellsCondemnedToDie

Immigration:
    make runVerySmallTwoColorsImm
    make runSmallTwoColorsImm
    make runSmallFourColoursImm
    make runMediumThreeColorsImm
    make runBigThreeColorsImm

Schelling:
	make runSchellingSmall2Colors
	make runSchellingSmall4Colors
	make runSchellingMedium3Colors
	make runSchellingMedium6Colors
	make runSchellingBig3Colors
	make runSchellingBig6Colors
Boids:
	make runTestBoids
